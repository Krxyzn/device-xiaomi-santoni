# DT2W
genfscon proc /sys/dev/dt2w    u:object_r:sysctl_dt2w:s0

# Leds
genfscon sysfs /devices/soc/soc:gpio-leds/leds/ u:object_r:sysfs_leds:s0
genfscon sysfs /devices/soc/78b6000.i2c/i2c-2/2-0045/leds/ u:object_r:sysfs_leds:s0
genfscon sysfs /devices/platform/soc/soc:gpio-leds/leds/ u:object_r:sysfs_leds:s0
genfscon sysfs /devices/platform/soc/78b6000.i2c/i2c-2/2-0045/leds/ u:object_r:sysfs_leds:s0
